{
  "name": "angular2-drag-scroll",
  "version": "2.0.0-alpha.0",
  "description": "Lightweight drag to scroll directive for Angular2",
  "contributors": [
    {
      "name": "Fan Jin",
      "email": "fanjin1989@gmail.com"
    }
  ],
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run ngc",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --single-run",
    "clean:build": "npm run rimraf -- src/**/*.js src/**/*.d.ts src/**/*.map src/**/*.metadata.json index.js index.d.ts index.js.map index.metadata.json",
    "clean": "npm run rimraf -- node_modules && npm cache clean",
    "rimraf": "rimraf",
    "ngc": "ngc -p src/tsconfig.json",
    "tsc": "tsc -p src",
    "build:all": "ng build --prod && ng build --prod --app 1",
    "build:aot": "ng build --prod --aot && ng build --prod --aot --app 1",
    "build:optimizer": "ng build --prod --aot --build-optimizer && ng build --prod --aot --build-optimizer --app 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfwg/angular2-drag-scroll.git"
  },
  "keywords": [
    "Angular 2",
    "drag",
    "scroll"
  ],
  "author": "Fan Jin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bfwg/angular2-drag-scroll.git"
  },
  "homepage": "https://github.com/bfwg/angular2-drag-scroll#readme",
  "peerDependencies": {
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/core": "4.3.6"
  },
  "devDependencies": {
    "@angular/animations": "4.3.6",
    "@angular/cdk": "2.0.0-beta.10",
    "@angular/cli": "1.4.1",
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/compiler-cli": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/forms": "4.3.6",
    "@angular/http": "4.3.6",
    "@angular/material": "2.0.0-beta.10",
    "@angular/platform-browser": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@angular/platform-server": "4.3.6",
    "@angular/router": "4.3.6",
    "@nguniversal/express-engine": "^1.0.0-beta.3",
    "@types/core-js": "^0.9.43",
    "@types/express": "^4.0.37",
    "@types/jasmine": "2.5.53",
    "@types/node": "6.0.88",
    "codelyzer": "3.1.1",
    "compression": "^1.7.0",
